unit Unit_Polymorphic;

interface
type  IColor=interface

      end;
      TAnimal=class(TInterfacedObject)
      public
        procedure Run();virtual;abstract;
      end;
      TDog=class(TAnimal,IColor)
        procedure Run();override;
      end;
      TCat=class(TAnimal,IColor)
        procedure Run();override;
      end;
implementation

{ TDog }

procedure TDog.Run;
begin
  Writeln('π∑‘⁄≈‹');
end;

{ TCat }

procedure TCat.Run;
begin
  Writeln('√®‘⁄≈‹');
end;

end.
